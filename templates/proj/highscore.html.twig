{% extends "projbase.html.twig" %}

{% block title %}Plump{% endblock %}

{% block content %}

<h1>Highscores</h1>

<table>
    <tr>
        <th>Name</th>
        <th>Score</th>
    </tr>
    {% for score in scores %}
        <tr>
            <td>
                {{ score.name }}
            </td>
            <td>
                {{ score.score }}
            </td>
        </tr>
    {% endfor %}
</table>


{% endblock %}
